#. type: delimited block -
#: upstream/_versions/2.13/guides/mongodb.adoc:159
#, no-wrap
msgid ""
"@ApplicationScoped\n"
"public class FruitService {\n"
msgstr ""

#. type: delimited block -
#: upstream/_versions/2.13/guides/mongodb.adoc:161
#: upstream/_versions/2.13/guides/mongodb.adoc:552
#, no-wrap
msgid "    @Inject MongoClient mongoClient;\n"
