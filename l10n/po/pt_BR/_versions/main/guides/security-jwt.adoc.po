#. type: Title ===
#: _versions/main/guides/security-jwt.adoc
#, no-wrap
msgid "MicroProfile JWT configuration"
msgstr ""

#: _versions/main/guides/security-jwt.adoc
msgid "Property Name"
msgstr ""

#. type: Table
#: _versions/main/guides/security-jwt.adoc
#, no-wrap
msgid "Default"
msgstr ""

