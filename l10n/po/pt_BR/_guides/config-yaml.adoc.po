msgstr "application.yaml"

#. type: Block title
#: upstream/_guides/config-yaml.adoc:147
#, fuzzy, no-wrap
msgid "application-staging.yaml"
msgstr "application-staging.yaml"

#. type: delimited block -
#: upstream/_guides/config-yaml.adoc:153
#, no-wrap
