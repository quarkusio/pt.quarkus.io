#. type: delimited block -
#: upstream/_guides/rabbitmq-reference.adoc:314
#, no-wrap
msgid ""
"import jakarta.enterprise.context.ApplicationScoped;\n"
"import jakarta.transaction.Transactional;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/rabbitmq-reference.adoc:317
#, no-wrap
msgid ""
"@ApplicationScoped\n"
"public class PriceStorage {\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/rabbitmq-reference.adoc:326
#, no-wrap
msgid ""
"    @Incoming(\"prices\")\n"
"    @Blocking\n"
"    @Transactional\n"
"    public void store(int priceInUsd) {\n"
"        Price price = new Price();\n"
"        price.value = priceInUsd;\n"
"        price.persist();\n"
"    }\n"
msgstr ""

#. type: delimited block =
#: upstream/_guides/rabbitmq-reference.adoc:333
#, fuzzy
msgid "There are 2 `@Blocking` annotations:"
msgstr "Existem 2 anotações em `@Blocking`:"

