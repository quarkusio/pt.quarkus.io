#, no-wrap
msgid ""
"    @Override\n"
"    public RuntimeException toThrowable(Response response) {\n"
"        if (response.getStatus() == 500) {\n"
"            response.readEntity(String.class); <2>\n"
"            return new RuntimeException(\"The remote service responded with HTTP 500\");\n"
