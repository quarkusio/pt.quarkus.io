#. type: Title ==
#: upstream/_guides/redis.adoc:623
#, fuzzy, no-wrap
msgid "Working with Binary Data"
msgstr "Trabajar con datos binarios"

#. type: Plain text
#: upstream/_guides/redis.adoc:630
#, fuzzy
msgid "While `RedisClient` and `ReactiveRedisClient` expose convenient `String`-based methods that mirror the commands available in Redis, they do not handle binary data.  In order to store binary data in Redis, you can inject an instance of the lower-level `MutinyRedis` class:"
msgstr "Mientras que `RedisClient` y `ReactiveRedisClient` exponen convenientes métodos basados en `String` que reflejan los comandos disponibles en Redis, no manejan datos binarios. Para almacenar datos binarios en Redis, puedes inyectar una instancia de la clase de nivel inferior `MutinyRedis`:"

#. type: delimited block -
#: upstream/_guides/redis.adoc:639
#, no-wrap
msgid ""
"import io.quarkus.redis.client.runtime.MutinyRedis;\n"
"import io.vertx.mutiny.redis.client.Command;\n"
"import io.vertx.mutiny.redis.client.Request;\n"
"import io.vertx.mutiny.redis.client.Response;\n"
"import javax.enterprise.context.ApplicationScoped;\n"
"import javax.inject.Inject;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/redis.adoc:642
#, no-wrap
msgid ""
"@ApplicationScoped\n"
"class BinaryCacheService {\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/redis.adoc:645
#, no-wrap
msgid ""
"    @Inject\n"
"    MutinyRedis mutinyRedis; // <1>\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/redis.adoc:654
#, no-wrap
msgid ""
"    Uni<Void> set(String key, byte[] data) {\n"
"        return mutinyRedis\n"
"            .send( // <2>\n"
"                Request.cmd(Command.SET)\n"
"                    .arg(key)\n"
"                    .arg(data)) // <3>\n"
"            .replaceWithVoid();\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/redis.adoc:664
#, no-wrap
msgid ""
"    Uni<byte[]> get(String key) {\n"
"        return mutinyRedis\n"
"            .send(\n"
"                Request.cmd(Command.GET)\n"
"                    .arg(key))\n"
"            .ifNotNull()\n"
"            .transform(Response::toBytes); // <4>\n"
"    }\n"
"}\n"
msgstr ""

