#: upstream/_guides/amqp-reference.adoc:102
#: upstream/_guides/amqp-reference.adoc:123
#, no-wrap
msgid "package inbound;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/amqp-reference.adoc:104
#, no-wrap
msgid "import org.eclipse.microprofile.reactive.messaging.Incoming;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/amqp-reference.adoc:106
#, no-wrap
msgid "import javax.enterprise.context.ApplicationScoped;\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/amqp-reference.adoc:109
#, no-wrap
msgid ""
"@ApplicationScoped\n"
"public class AmqpPriceConsumer {\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/amqp-reference.adoc:114
#, no-wrap
msgid ""
"    @Incoming(\"prices\")\n"
"    public void consume(double price) {\n"
"        // process your price.\n"
"    }\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/amqp-reference.adoc:116
#: upstream/_guides/amqp-reference.adoc:142
#: upstream/_guides/amqp-reference.adoc:399
#, no-wrap
msgid "}\n"
msgstr ""

#. type: Plain text
#: upstream/_guides/amqp-reference.adoc:119
msgid "Or, you can retrieve the Message<Double>:"
