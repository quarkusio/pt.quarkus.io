"import io.vertx.core.json.JsonObject;\n"
"//\n"
"@ApplicationScoped\n"
"public static class Consumer {\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/amqp-reference.adoc:189
#, no-wrap
msgid "    List<Price> prices = new CopyOnWriteArrayList<>();\n"
msgstr ""

#. type: delimited block -
#: upstream/_guides/amqp-reference.adoc:195
#, no-wrap
